      <div class="container_crud">
        <button type="button" onclick="Abrir_Modal()" class="btn btn-primary m-3">Nuevo</button>
        <div class="container">
            <table class= "table mt-2 table-bordered table-striped">
                <thead>
                    <tr class="text-center">
                    <th>Nº</th>
                    <th>Fecha</th>
                    <th>Tema</th>
                    <th>Duracion</th>
                    <th>Calificacion</th>
                    <th>IdAsesor</th>
                    <th>IdCliente</th>
                    <th>CorreoCliente</th>
                    <th>NombreCliente</th>
                    <th>Acciones</th>
                    </tr>
                </thead>
    
                <tbody id="tabla"></tbody>
            </table>
        </div>
        <div class="modal fade" id="MyModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Registrar Asesoria</h5>  
                  <button id="salir" type="button" class="btn-close" data-bs-dismiss="modal" style="display: block;" aria-label="Close" ></button>
                </div>
                <div class="modal-body">
                    <form>
                        <p ><b>Todos los campos son obligatorios</b></p>
                        <div class="mb-3">
                           <input type="hidden" class="form-control" id="id">
                       </div> 
                           <div class="mb-3">
                               <label for="recipient-name" class="col-form-label" >Fecha</label>
                               <input type="date" class="form-control" id="fecha" >
                             </div>
                           <div class="mb-3">
                               <label for="recipient-name" class="col-form-label">Tema</label>
                               <input type="text" class="form-control" id="tema">
                           </div>
                           <div class="mb-3">
                             <label for="recipient-name" class="col-form-label">Duracion</label>
                             <input type="time" class="form-control" id="duracion">
                         </div>
                           <div class="mb-3">
                               <label for="recipient-name" class="col-form-label">Calificacion</label>
                                    <select class="form-select" name="calificacion" id="calificacion" required>
                                    <option selected disabled value="">Calificacion</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                    </select>
                           </div>
                           <div class="mb-3">
                               <label for="recipient-name" class="col-form-label">Nombre del asesor</label>
                               <select class="form-select" name="IdAsesor" id="idAsesor" required>
                               <option selected disabled value="">Seleccione</option>
                               <option value="14">Juan Yuco</option>
                               <option value="12">felipe</option>
                               </select>
                           </div>
                           <div class="mb-3">
                               <label for="recipient-name" class="col-form-label"> Identificacion</label>
                               <input type="number" class="form-control" id="idcliente" minlength="10" maxlength="10">
                           </div>
                           <div class="mb-3">
                               <label for="recipient-name" class="col-form-label">CorreoCliente</label>
                               <input type="email" class="form-control" id="correo">
                           </div>
                           <div class="mb-3">
                             <label for="recipient-name" class="col-form-label">NombreCliente</label>
                             <input type="text" class="form-control" id="nombre">
                         </div>
                          <!--  <div class="modal-footer">
                               <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                               <button type="button" class="btn btn-primary" data-bs-dismiss="modal" onclick="registrar_Asesoria()">Guardar</button>
                               <button type="button" class="btn btn-primary" data-bs-dismiss="modal" onclick="Actualizar_Asesoria()">Actualizar</button>
                           </div> -->
                       </form>
                  </div> 
                <div class="modal-footer">
                  <button id="actualizar" type="button" class="btn btn-primary" style="display: block;" onclick="Actualizar_Asesoria()">Actualizar</button>
                  
                  <button id="close"  type="button"  class="btn btn-secondary" style="display: block;" data-bs-dismiss="modal" >Close</button>
                  <button id="registrar" type="button" class="btn btn-primary" style="display: block;" onclick="registrar_Asesoria()">Registrar</button>
